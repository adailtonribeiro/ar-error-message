{
  "name": "ar-error-message",
  "version": "0.4.4-alpha.0",
  "description": "A Simple Ionic Error Message Module",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ngc": "ngc",
    "build": "npm run ngc",
    "publishPackage": "npm run build && npm publish",
    "prettier": "prettier --write src/**",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prerelease-alpha": "npm run release -- --prerelease alpha",
    "release-patch": "npm run release -- --release-as patch",
    "release-minor": "npm run release -- --release-as minor",
    "release-major": "npm run release -- --release-as major",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adailtonribeiro/ar-error-message.git"
  },
  "keywords": [
    "ionic module",
    "error message",
    "message component"
  ],
  "author": "Adailton Ribeiro",
  "license": "MIT",
  "homepage": "http://www.adailtonribeiro.com",
  "devDependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "husky": "^0.14.3",
    "ionic-angular": "^3.9.2",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "rxjs": "6.1.0",
    "standard-version": "^4.3.0",
    "typescript": "^2.7.2",
    "zone.js": "0.8.26"
  }
}
